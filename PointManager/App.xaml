<Application x:Class="PointManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PointManager"
             xmlns:converter="clr-namespace:PointManager.Converters"
             xmlns:viewModel="clr-namespace:PointManager.ViewModels"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <converter:CameraPositionConverter x:Key="cameraPositionConverter" />
            <viewModel:ViewModelLocator x:Key="ViewModelLocator"></viewModel:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
